; Prologue
ADD R6, R6, #-1    ; Push RA
STR R7, R6, #0
ADD R6, R6, #-1    ; Push FP  
STR R5, R6, #0
ADD R5, R6, #-1    ; Set new FP
ADD R6, R6, #-n    ; Allocate locals

; Your code here

; Epilogue  
ADD R6, R5, #1     ; Deallocate
LDR R5, R6, #0     ; Restore FP
ADD R6, R6, #1
LDR R7, R6, #0     ; Restore RA
ADD R6, R6, #1
RET
