.ORIG x3000
LD R6, STACK_BASE
LD R0, ISR_SET
STI R0, KEYBOARD
TOP LD R0, ASCII_0
AND R1, R1, #0
ADD R1, R1, #10
PRINT_NUM ADD R0, R0, #1
OUT
ADD R1, R1, #-1
BRp PRINT_NUM
LD R0, NEWLINE
OUT
BRnzp TOP
NEWLINE .FILL #10
ASCII_0 .FILL #47
STACK_BASE .FILL x3050
KEYBOARD .FILL xFE00
ISR_SET .FILL x4000
.END
